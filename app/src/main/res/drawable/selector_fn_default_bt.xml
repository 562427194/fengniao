<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:width="1px" android:color="@color/def_bt_pressed"></solid>
            <corners android:radius="3dp"></corners>
        </shape>
    </item>
    <item >
        <shape>
            <solid android:width="1px" android:color="@color/tab_text_checked"></solid>
            <corners android:radius="3dp"></corners>
        </shape>
    </item>
</selector>