<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.visionet.fengniao">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".FNApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon,android:theme">
        <activity
            android:name=".activity.MainActivity"
            android:theme="@style/AppTheme"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ProductDetailActivity"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".activity.InsuranceDetailActivity"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".activity.InsureActivity"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".activity.NoticeActivity"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>